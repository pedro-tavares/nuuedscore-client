<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.1//EN" "http://gwtproject.org/doctype/2.8.1/gwt-module.dtd">
<module rename-to='nuuedscore'>
	
	<!-- Inherit the core Web Toolkit stuff -->
	<inherits name='com.google.gwt.core.Core' />
	<inherits name='com.google.gwt.user.User' />

	<!-- Inherit a core Web Toolkit theme -->
	<!-- <inherits name='com.google.gwt.user.theme.clean.Clean' /> -->
	<!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
	<inherits name='com.google.gwt.user.theme.chrome.Chrome'/>
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->
		
	<inherits name="org.fusesource.restygwt.RestyGWT" />
	<inherits name="com.google.gwt.json.JSON" />
	
	<!-- Other module inherits -->
	
	<!-- Entry point class. -->
	<entry-point class='com.nuuedscore.client.NuuEdScore' />

	<!-- Paths for translatable code -->
	<source path='client' />
	<source path='shared' />

	<!-- allow Super Dev Mode -->
	<!--  
	<add-linker name="xsiframe" />
	-->
	
	<set-property name="user.agent" value="gecko1_8,safari"/>
	
</module>